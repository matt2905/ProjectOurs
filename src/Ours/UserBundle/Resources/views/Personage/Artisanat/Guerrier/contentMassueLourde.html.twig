<table id="soMassueLo" class="table table-bordered artisanat">
    <thead>
    <tr>
        <th colspan="2" class="eso">MASSUES LOURDE</th>
    </tr>
    <tr>
        <th>Pseudo</th>
        <th class="tier">Tier</th>
    </tr>
    </thead>

    <tbody>
    <tr>
        <td colspan="2" class="null"></td>
    </tr>
    {% for personage in personages %}
        {% if personage.soMassueLo %}
            <tr>
                <td class="eso">{{ personage.name }}</td>
                {% if personage.soMassueLo == 4.2 %}
                    <td class="quatrePdeux tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 5.2 %}
                    <td class="cinqPdeux tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 6.2 %}
                    <td class="sixPdeux tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 7.2 %}
                    <td class="septPdeux tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 8.2 %}
                    <td class="huitPdeux tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 5.5 %}
                    <td class="cinqPcinq tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 6.5 %}
                    <td class="sixPcinq tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 7.5 %}
                    <td class="septPcinq tier">{{ personage.soMassueLo }}</td>
                {% elseif personage.soMassueLo == 8.5 %}
                    <td class="huitPcinq tier">{{ personage.soMassueLo }}</td>
                {% else %}
                    <td class="deux tier">{{ personage.soMassueLo }}</td>
                {% endif %}
            </tr>
        {% endif %}
    {% endfor %}
    </tbody>
</table>