    {# Outils & Accessoire #}
    <div id="divers" class="tab-pane fade in active">
        {# Outils #}
        <div>
            <h3>Outils</h3>
            {{ render(controller('OursUserBundle:Artisanat\\Outils:Hache', { 'page': 0, "json": false})) }}
