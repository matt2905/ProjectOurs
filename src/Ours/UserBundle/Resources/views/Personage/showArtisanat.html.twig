{% extends '::base.html.twig' %}

{% block title %}Artisanat{% endblock %}

{% block stylesheets %}
    {% stylesheets '@OursUserBundle/Resources/public/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {%  endstylesheets %}
    <style>
        body, td.null, th.null
        {
            background-color: #CCCCCC;
            border-color: #CCCCCC !important;
        }
        td, th
        {
            border-color: #FFFFFF !important;
        }
    </style>
{% endblock %}

{% block body -%}

<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#divers">Outils & Accessoire</a></li>
    <li><a data-toggle="tab" href="#mage">Tour du mage</a></li>
    <li><a data-toggle="tab" href="#chasseur">Pavillon du chasseur</a></li>
    <li><a data-toggle="tab" href="#soldat">Forge du guerrier</a></li>
</ul>

