<table id="soEpeeLarge" class="table table-bordered artisanat">
    <thead>
    <tr>
        <th colspan="2" class="fire">EPEE LARGE</th>
    </tr>
    <tr>
        <th>Pseudo</th>
        <th class="tier">Tiers</th>
    </tr>
    </thead>

    <tbody>
    <tr>
        <td colspan="2" class="null"></td>
    </tr>
    {% for personage in personages %}
        {% if personage.soEpeeLarge %}
            <tr>
                <td class="fire">{{ personage.name }}</td>
                {% if personage.soEpeeLarge == 3 %}
                    <td class="trois tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 4 %}
                    <td class="quatre tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 5 %}
                    <td class="cinq tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 6 %}
                    <td class="six tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 7 %}
                    <td class="sept tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 8 %}
                    <td class="huit tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 5.4 %}
                    <td class="cinqPquatre tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 6.4 %}
                    <td class="sixPquatre tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 7.4 %}
                    <td class="septPquatre tier">{{ personage.soEpeeLarge }}</td>
                {% elseif personage.soEpeeLarge == 8.4 %}
                    <td class="huitPquatre tier">{{ personage.soEpeeLarge }}</td>
                {% else %}
                    <td class="deux tier">{{ personage.soEpeeLarge }}</td>
                {% endif %}
            </tr>
        {% endif %}
    {% endfor %}
    </tbody>
</table>