<table id="alHeal" class="table table-bordered agriculture">
    <thead>
    <tr>
        <th colspan="2" class="heal">SOIN</th>
    </tr>
    <tr>
        <th>Pseudo</th>
        <th class="tier">Tier</th>
    </tr>
    </thead>

    <tbody>
    <tr>
        <td colspan="2" class="null"></td>
    </tr>
    {% for personage in personages %}
        {% if personage.alHeal %}
            <tr>
                <td class="heal">{{ personage.name }}</td>
                {% if personage.alHeal == 3 %}
                    <td class="trois tier">{{ personage.alHeal }}</td>
                {% elseif personage.alHeal == 4 %}
                    <td class="quatre tier">{{ personage.alHeal }}</td>
                {% elseif personage.alHeal == 5 %}
                    <td class="cinq tier">{{ personage.alHeal }}</td>
                {% elseif personage.alHeal == 6 %}
                    <td class="six tier">{{ personage.alHeal }}</td>
                {% elseif personage.alHeal == 7 %}
                    <td class="sept tier">{{ personage.alHeal }}</td>
                {% elseif personage.alHeal == 8 %}
                    <td class="huit tier">{{ personage.alHeal }}</td>
                {% else %}
                    <td class="deux tier">{{ personage.alHeal }}</td>
                {% endif %}
            </tr>
        {% endif %}
    {% endfor %}
    </tbody>
</table>